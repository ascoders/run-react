{
    "compilerOptions": {
        "baseUrl": ".",
        "module": "commonjs",
        "noImplicitAny": false,
        "target": "es6",
        "removeComments": true,
        "preserveConstEnums": true,
        "jsx": "react",
        "sourceMap": true,
        "outDir": "built",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "exclude": [
        "node_modules",
        "built"
    ]
}